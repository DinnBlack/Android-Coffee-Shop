<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.AdminOrderActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tbOrderHistory"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:background="#ffffff"
        android:elevation="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title=" " />

    <nl.joery.animatedbottombar.AnimatedBottomBar
        android:id="@+id/navOrderHistory"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:background="#ffffff"
        app:abb_indicatorAppearance="round"
        app:abb_indicatorColor="#000000"
        app:abb_indicatorHeight="5dp"
        app:abb_indicatorMargin="90dp"
        app:abb_selectedIndex="0"
        app:abb_selectedTabType="text"
        app:abb_tabColor="#5E5E5E"
        app:abb_tabColorSelected="#000000"
        app:abb_tabs="@menu/menu_admin_bottom_nav"
        app:backgroundTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="64dp"
        android:layout_marginBottom="64dp"
        app:layout_constraintBottom_toBottomOf="@+id/botnavBar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tbOrderHistory">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>